<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4"
         xmlns="http://java.sun.com/xml/ns/j2ee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
         http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

<servlet>
    <servlet-name>startPage</servlet-name>
    <jsp-file>/content.jsp</jsp-file>
</servlet>
<servlet-mapping>
    <servlet-name>startPage</servlet-name>
    <url-pattern>/start</url-pattern>
</servlet-mapping>
    <servlet>
        <servlet-name>login</servlet-name>
        <jsp-file>/login.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>def</servlet-name>
        <servlet-class>start.Start</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>def</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>check</servlet-name>
        <servlet-class>start.Check</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>check</servlet-name>
        <url-pattern>/check</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>exit</servlet-name>
        <servlet-class>exit.Exit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>exit</servlet-name>
        <url-pattern>/exit</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>db</servlet-name>
        <servlet-class>db.Db</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>db</servlet-name>
        <url-pattern>/db</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>infoFromDB</servlet-name>
        <jsp-file>/infoFromDB.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>infoFromDB</servlet-name>
        <url-pattern>/info</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>download</servlet-name>
        <servlet-class>db.ServletDownload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>download</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>charts</servlet-name>
        <jsp-file>/chart.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>charts</servlet-name>
        <url-pattern>/chart</url-pattern>
    </servlet-mapping>
</web-app>